docker run --rm -t -i \
  -v /srv/gitlab-runner/config:/etc/gitlab-runner \
  --name gitlab-runner \
  gitlab/gitlab-runner register \
  --non-interactive \
  --executor "docker" \
  --docker-privileged \
  --docker-image docker:latest \
  --url "https://gitlab.commandemy.training/" \
  --registration-token "pCs-UhiwB23ns-_d1yce" \
  --description "docker-runner" \
  --tag-list "docker" \
  --run-untagged \
  --locked="false"
